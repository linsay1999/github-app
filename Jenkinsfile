pipeline {
    agent any

    stages {
        stage('Hello1') {
            steps {
                sh '''
                date
                '''
            }
        }

        stage('Hello2') {
            steps {
                sh '''
                cat /etc/passwd
                '''
            }
        }

        stage('Hello3') {
            steps {
                sh '''
                ls -l
                '''
            }
        }

        stage('Hello4') {
            steps {
                sh '''
                lsblk
                '''
            }
        }

        stage('Hello5') {
            steps {
                sh '''
                pwd
                '''
            }
        }










    }













}
